<!DOCTYPE html>
<html class="no-js">
<head>
    <title>@Page.Title</title>
    <link href="@Href("~/Styles/Site.css")" rel="stylesheet" />
    <link href="@Href("~/Styles/Grid960.css")" rel="stylesheet"/>
    <link href="@Href("~/favicon.ico")" rel="shortcut icon" type="image/x-icon" />
    <script src="/Scripts/jquery-1.7.min.js" type="text/javascript"></script>
    <script src="/Scripts/modernizr-2.0.6-development-only.js" type="text/javascript"></script>
    <meta charset="utf-8" />
    @RenderSection("head", required: false)
</head>
<body>
    <div id="header">
        <h1>
            <a href="@Href("~/")">ASP.NET GoogleMap Control</a>
        </h1>
        <ul id="menu" class="@Page.CssClass">
            <li class="home"><a href="@Href("~/")">home</a></li>
            <li class="project"><a href="http://googlemap.codeplex.com/">project</a></li>
            <li class="demo"><a href="@Href("~/samples.aspx")">samples</a></li>
            <li class="docs"><a href="@Href("~/docs")">api docs</a></li>
            <li class="donate"><a href="@Href("~/donate")">donate</a></li>
            <li class="contact"><a href="@Href("~/contact")">contact</a></li>
        </ul>
    </div>
    <div id="teaser">
        <div class="wrap">
            <div id="image">
            </div>
            <div class="box">
                <h2>
                    Welcome to <em title="Wraps Google Maps API as ASP.NET custom control">GogleMap Control v6.0</em></h2>
                <p>
                    The GoogleMap Control project wraps Google Maps API as ASP.NET Custom Control, providing
                    an easy and fast way of adding Google Maps in your ASP.NET pages with "Zero-Written-JavaScript"
                    code.<br />
                    GoogleMap Control includes many features (markers, polyline, poligons, directions)
                    and gives you almost all Google Maps API functionality in enjoyable ASP.NET way
                    of coding.
                </p>
            </div>
        </div>
    </div>
    <div id="bar">
        <div class="wrap">
            <a class="step" href="http://googlemap.codeplex.com/releases" target="_blank">1<span>download</span></a>
            <a class="step" href="http://googlemap.codeplex.com/wikipage?title=Installation&referringTitle=Documentation"
                target="_blank">2 <span>setup</span></a> <a class="step" href="http://googlemap.codeplex.com/documentation"
                    target="_blank">3<span>get started</span></a>
        </div>
    </div>
    <div class="wrap">
        <div id="content" class="container_12">
            <div class="content">
                <div class="grid_9 alpha">
                    @RenderBody()
                </div>
                <div class="grid_3 omega side">
                    @*<h3>
                        Ads
                    </h3>*@
                    <h3>
                        Videos
                    </h3>
                    <ul>
                        <li><a href="http://artembg.com/video/googlemap-setup" target="_blank">&raquo; GoogleMap
                            Setup</a></li>
                        <li><a href="http://artembg.com/video/googlemap-geocoding" target="_blank">&raquo; GoogleMap
                            Geocoding</a></li>
                        <li><a href="http://artembg.com/video/googlemap-capture" target="_blank">&raquo; GoogleMap
                            Capture</a></li>
                        <li><a href="http://artembg.com/video/GoogleMarkerInfoWindow" target="_blank">&raquo;
                            GoogleMarker InfoWindow</a></li>
                        <li><a href="http://artembg.com/video/GoogleMapControl-MarkerManager" target="_blank">
                            &raquo; GoogleMap MarkerManager</a></li>
                        <li><a href="http://artembg.com/video/GoogleMapUsingSkin" target="_blank">&raquo; GoogleMap
                            "Look&Feel" </a></li>
                        <li><a href="http://artembg.com/video/GoogleMapReverseGeocoding" target="_blank">&raquo;
                            GoogleMap Reverse Geocoding</a></li>
                        <li><a href="http://artembg.com/video/GoogleMapAddressNotFound" target="_blank">&raquo;
                            GoogleMap "Address Not Found"</a></li>
                        <li><a href="http://artembg.com/videos/GoogleMap-Control" target="_blank">&raquo; All
                            Videos</a></li>
                    </ul>
                    <h3>
                        Links
                    </h3>
                    <ul>
                        <li><a href="http://googlemap.codeplex.com/" target="_blank">&raquo; Project Home</a></li>
                        <li><a href="http://code.google.com/apis/maps/" target="_blank">&raquo; Google Maps
                            API</a></li>
                        <li><a href="http://artembg.com/paypal.donate?p=googlemap" target="_blank">&raquo; Donate
                            Project</a></li>
                        <li><a href="http://artembg.com" target="_blank">&raquo; ArtemBG WebSite</a></li>
                    </ul>
                    <div style="margin-top: 20px;">
                        <a href="http://lakequincy.com?" title="Advertise on this site through Lake Quincy Media"
                            target="_blank">
                            <img src="http://lakequincy.com/images/LQM_advertise_here_button.gif" alt="Advertise on this site through Lake Quincy Media"
                                width="135" height="90" border="0" /></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="footer"> <p class="right"> Powered by <a href="http://www.orchardproject.net/">Orchard</a>.
    Design by <a href="http://www.solucija.com/"> Luka Cvrk</a> </p> <p> &copy; Copyright
    @DateTime.Now.Year <a href="http://artembg.com/">Velio Ivanov</a> </p> </div>
</body>
</html>
